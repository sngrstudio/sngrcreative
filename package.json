{
  "name": "sngrcreative",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "astro": "astro",
    "build": "conc bun:check:* && astro build",
    "cf-typegen": "wrangler types",
    "check:astro": "astro check",
    "check:style": "prettier --check .",
    "deploy": "conc bun:build && wrangler pages deploy",
    "dev": "astro dev --host=0.0.0.0",
    "format": "prettier --write .",
    "preview": "conc bun:build && wrangler pages dev --ip=0.0.0.0",
    "start": "conc bun:preview"
  },
  "dependencies": {
    "@astrojs/check": "0.9.4",
    "@astrojs/cloudflare": "12.6.9",
    "@astrojs/react": "4.4.0",
    "@keystatic/astro": "5.0.6",
    "@keystatic/core": "0.5.48",
    "@tailwindcss/vite": "4.1.14",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "tailwindcss": "4.1.14"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20251004.0",
    "@types/bun": "1.2.23",
    "@types/react": "19.2.0",
    "@types/react-dom": "19.2.0",
    "astro": "5.7.4",
    "concurrently": "9.2.1",
    "prettier": "3.6.2",
    "prettier-config-standard": "7.0.0",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-tailwindcss": "0.6.14",
    "typescript": "5.9.3",
    "wrangler": "4.42.0"
  }
}
