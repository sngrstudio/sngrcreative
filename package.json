{
  "name": "sngrcreative",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "astro": "astro",
    "build": "conc bun:check:* && astro build",
    "cf-typegen": "wrangler types",
    "check:astro": "astro check",
    "check:style": "prettier --check .",
    "deploy": "conc bun:build && wrangler pages deploy",
    "dev": "astro dev --host=0.0.0.0",
    "format": "prettier --write .",
    "preview": "conc bun:build && wrangler pages dev --ip=0.0.0.0",
    "start": "conc bun:preview"
  },
  "dependencies": {
    "@astrojs/check": "0.9.4",
    "@astrojs/cloudflare": "12.5.4",
    "@astrojs/react": "4.3.0",
    "@keystatic/astro": "5.0.6",
    "@keystatic/core": "0.5.46",
    "@tailwindcss/vite": "4.1.7",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwindcss": "4.1.7"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20250416.0",
    "@types/bun": "1.2.15",
    "@types/react": "19.1.6",
    "@types/react-dom": "19.1.5",
    "astro": "5.9.1",
    "concurrently": "9.1.2",
    "prettier": "3.5.3",
    "prettier-config-standard": "7.0.0",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-tailwindcss": "0.6.11",
    "typescript": "5.8.3",
    "wrangler": "4.11.1"
  }
}
